<div id='graphs'>
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});
      google.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = new google.visualization.DataTable();
        data.addColumn('string', 'Tables');
        data.addColumn('number', 'Block Index Memory');
        data.addColumn('number', 'Bloom Filter Memory');
        data.addRows(3);
        data.setValue(0, 0, 'Meta Data');
        data.setValue(0, 1, 0);
        data.setValue(1, 0, 'Load Test');
        data.setValue(1, 1, 5135);
        data.setValue(1, 2, 51811);
        data.setValue(2, 0, 'Load Test');
        data.setValue(2, 1, 5135);
        data.setValue(2, 2, 51811);

        var chart = new google.visualization.BarChart(document.getElementById('graph'));
        chart.draw(data, {width: 900, height: 240, title: 'Tables',
                          vAxis: {title: 'Year', titleColor: 'red'}
                         });
      }
    </script>

<div id='graph' class='graph'>
<div>
</div>